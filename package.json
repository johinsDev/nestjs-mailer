    "bull": "^4.10.2",
    "classnames": "^2.3.2",
    "luxon": "^3.1.1",
    "mailing-core": "^0.9.8",
    "mjml-react": "^2.0.8",
    "next": "^13.1.2",
    "nodemailer": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
